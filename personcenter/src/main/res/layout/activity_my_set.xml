<?xml version="1.0" encoding="utf-8" standalone="no"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">
<LinearLayout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@mipmap/homepage_bg"
    android:layout_height="match_parent"
    android:orientation="vertical"
   >
    <LinearLayout
        android:layout_width="match_parent"
        android:background="#4D000000"
        android:orientation="vertical"
        android:paddingBottom="20dp"
        android:layout_height="wrap_content">
        <include
            android:id="@+id/include"
            layout="@layout/personcenter_head"/>
    </LinearLayout>
<LinearLayout
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="wrap_content">
<LinearLayout
    android:layout_width="match_parent"
    android:orientation="horizontal"
    android:paddingTop="5dp"
    android:background="@mipmap/person_ti_item"
    android:paddingBottom="5dp"
    android:paddingStart="10dp"
    android:layout_marginStart="20dp"
    android:layout_marginEnd="20dp"
    android:layout_height="wrap_content">
    <it.mbkj.kuangji.view.NewTextView
        android:text="修改交易密码"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_gravity="center"
        android:textColor="@color/libTextWhite"
        android:layout_height="wrap_content"/>
    <it.mbkj.kuangji.view.NewTextView
        android:id="@+id/tv_xiugaijiaoyi"
        android:layout_width="wrap_content"
        android:background="@mipmap/person_qianwang"
        android:layout_gravity="center"
        android:layout_marginEnd="20dp"
        android:layout_height="wrap_content"/>

</LinearLayout>
    <LinearLayout
    android:layout_width="match_parent"
    android:orientation="horizontal"
    android:paddingTop="5dp"
    android:layout_marginTop="10dp"
    android:layout_marginStart="20dp"
    android:layout_marginEnd="20dp"
    android:background="@mipmap/person_ti_item"
    android:paddingBottom="5dp"
    android:paddingStart="10dp"
    android:layout_height="wrap_content">
    <it.mbkj.kuangji.view.NewTextView
        android:layout_width="0dp"
        android:layout_weight="1"
        android:text="修改登录密码"
        android:layout_gravity="center"
        android:textColor="@color/libTextWhite"
        android:layout_height="wrap_content"/>
        <it.mbkj.kuangji.view.NewTextView
            android:id="@+id/tv_xiugaidenglu"
            android:layout_width="wrap_content"
            android:background="@mipmap/person_qianwang"
            android:layout_gravity="center"
            android:layout_marginEnd="20dp"
            android:layout_height="wrap_content"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:paddingTop="5dp"
        android:layout_marginTop="10dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:background="@mipmap/person_ti_item"
        android:paddingBottom="5dp"
        android:paddingStart="10dp"
        android:layout_height="wrap_content">
        <it.mbkj.kuangji.view.NewTextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:text="版本号"
            android:layout_gravity="center"
            android:textColor="@color/libTextWhite"
            android:layout_height="wrap_content"/>
        <it.mbkj.kuangji.view.NewTextView
            android:id="@+id/tv_banben"
            android:layout_width="wrap_content"
           android:text="1.0.0"
            android:textColor="@color/libTextWhite"
            android:layout_gravity="center"
            android:layout_marginEnd="20dp"
            android:layout_height="wrap_content"/>

    </LinearLayout>
    <it.mbkj.kuangji.view.NewTextView
        android:id="@+id/tv_tuichu"
        android:layout_width="wrap_content"
        android:layout_marginTop="50dp"
        android:layout_gravity="center"
        android:gravity="center"
        android:background="@mipmap/person_tuichu"
        android:layout_height="wrap_content"/>
</LinearLayout>
</LinearLayout>
</layout>